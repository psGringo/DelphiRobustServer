unit uMains;

interface

uses
  uMain, uRSService;

type
  TMains = class
    class function GetMain: TMain;
  end;

implementation


{ TMains }

class function TMains.GetMain: TMain;
begin
  if Assigned(RobustService) then
    Result := RobustService.MainInstance
  else
    Result := Main;
end;

end.

